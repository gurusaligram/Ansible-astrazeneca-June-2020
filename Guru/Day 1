ansible localhost -m group -a "name=deploy state=present"
ansible localhost -m user -a "name=deploy-user state=present group=deploy shell=/bin/bash"
ansible localhost -m service -a "name=httpd state=restarted enabled=yes"
ansible localhost -m copy -a"src=index.html dest=/var/www/html/second.html"
ansible localhost -m yum -a "name=git 
ansible localhost -m yum -a "name=wget"
ansible localhost -m git -a "repo=https://github.com/scmgalaxy/ansible-role-template clone=no"
ansible localhost -m "reboot"
ansible localhost -m file -a "path=/opt/devopsschool.txt state=touch"
